logging {
  level  = "info"
  format = "logfmt"
}

remotecfg {
    url = {GRAFANA_FLEET_URL}
    basic_auth {
        username      = {GRAFANA_FLEET_USERNAME}
        password      = {GRAFANA_FLEET_PASSWORD}
    }

    id             = constants.hostname
    attributes     = {"cluster" = "dev", "namespace" = "otlp-dev"}
    poll_frequency = "5m"
}
